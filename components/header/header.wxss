.header-title {
  flex: 1;
  padding: 0 16px;
  font-size: 17px;
  text-align: center;
}

.header-right {
  width: 56px;
}

.header.isx .header-wrap {
  padding-top: 44px;
}

.header.isx .header-placeholder {
  height: 88px;
}

.header.android .header-placeholder {
  height: 72px;
}

.header.android .header-wrap {
  height: 48px;
