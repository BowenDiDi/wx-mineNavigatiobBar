.header {
  width: 100%;
}

.header-placeholder {
  width: 100%;
  height: 64px;
}

.header-wrap {
  top: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 44px;
  padding-top: 20px;
}

.header-wrap .back {
  position: relative;
  width: 24px;
  height: 24px;
  padding: 0;
  margin: 0 0 0 16px;
  /*color: inherit;*/
  /*background-color: transparent;*/
}
.header-wrap .home {
  position: relative;
  width: 24px;
  height: 24px;
  padding: 0;
  margin: 0 0 0 16px;
}

